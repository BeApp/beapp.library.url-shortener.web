{
    "name": "beapp/url-shortener",
    "version": "0.1.0",
    "type": "library",
    "description": "Short url generator using Doctrine orm",
    "keywords": ["doctrine", "url-shortener"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tanguy Reviller",
            "email": "t.reviller@beapp.fr"
        }
    ],
    "require": {
        "php": ">=7.1",
        "beapp/doctrine-extensions": "^0.2.0",
        "doctrine/orm": "^2.6",
        "symfony/config": "^4.2",
        "symfony/framework-bundle": "^4.2",
        "symfony/routing": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "symplify/monorepo-builder": "^4.7"
    },
    "autoload": {
        "psr-4": {
            "Beapp\\UrlShortener\\": "packages/Core/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Beapp\\UrlShortener\\": "packages/Core/tests"
        }
    },
    "replace": {
        "beapp/url-shortener-core": "self.version"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
